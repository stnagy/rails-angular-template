<!DOCTYPE html>
<html ng-app="rootApp">
  <head>
    <title>RootApp</title>
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular-material-icons/0.7.0/angular-material-icons.min.js"></script>
    <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/angular_material/1.1.0-rc2/angular-material.min.css">
  </head>
  <body layout="column" flex>
    <md-content flex layout="column" id="content">
      <div ui-view="main" flex layout="column"></div>
    </md-content>
  </body>
</html>
